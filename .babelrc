{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", [{ "style": "css", "libraryName": "antd-mobile" }]]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2", "react"],
      "plugins": [ "istanbul" ]
    }
  }
}
